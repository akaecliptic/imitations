{
	"compilerOptions": {
		"target": "esnext",
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"baseUrl": "./src",
	},
	"ts-node": {
		"require": ["tsconfig-paths/register"]
	},
	"include": [
		"src",
	],
	"exclude": [
		"node_modules"
	]
}
